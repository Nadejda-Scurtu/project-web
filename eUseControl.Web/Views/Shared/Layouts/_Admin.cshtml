<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Sport Wish</title>
    <link rel="icon" href="~/Content/images/favicon.ico">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery.perfect-scrollbar/1.5.2/css/perfect-scrollbar.min.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.perfect-scrollbar/1.5.2/perfect-scrollbar.min.js"></script>

    @Styles.Render("~/Content/bootstrap.min.css")
    @Styles.Render("~/Content/font-awesome.css")
    @Styles.Render("~/Content/demo.css")
    @Styles.Render("~/Content/jquery-ui.min.css")
    @Styles.Render("~/Content/paper-dashboard.css")


</head>
<body>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <div class="wrapper ">
        @Html.Partial("AdminLayouts/_Sidebar")

        <div class="main-panel">

            @Html.Partial("AdminLayouts/_Navbar")

            <div class="content">
                @RenderBody()
            </div>

            @Html.Partial("AdminLayouts/_Footer")
        </div>
    </div>



    <!--Section for main scripts render-->


    @Scripts.Render("~/Scripts/scr/jquery.min.js")
    @Scripts.Render("~/Scripts/scr/paper-dashboard.min.js")
    @Scripts.Render("~/Scripts/scr/popper.min.js")
    @Scripts.Render("~/Scripts/scr/bootstrap.min.js")

    @Scripts.Render("~/Scripts/scr/bootstrap-notify.js")
    @Scripts.Render("~/Scripts/scr/chartjs.min.js")
    @Scripts.Render("~/Scripts/scr/dropzone.js")
    @Scripts.Render("~/Scripts/scr/perfect-scrollbar.jquery.min.js")
    @Scripts.Render("~/Scripts/scr/demo.js")




    @RenderSection("jquery.min", required: false)
    @RenderSection("popper.min", required: false)
    @RenderSection("bootstrap.min", required: false)


    @RenderSection("animsition.min", required: false)
    @RenderSection("slick-custom", required: false)
    @RenderSection("slick", required: false)
    @RenderSection("isotope.pkgd.min", required: false)
    @RenderSection("sweetalert.min", required: false)
    @RenderSection("select2.min", required: false)

    @RenderSection("chartjs.min", required: false)
    @RenderSection("bootstrap-notify", required: false)
    @RenderSection("dropzone", required: false)
    @RenderSection("perfect-scrollbar.jquery.min", required: false)
    @RenderSection("paper-dashboard.min", required: false)
    @RenderSection("demo", required: false)

    <!--  Google Maps Plugin    -->
    <script src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>

    <script>
        $(document).ready(function () {
            // Javascript method's body can be found in assets/assets-for-demo/js/demo.js
            demo.initChartsPages();

            $('#my-dropzone').dropzone({
                url: '/file-upload'
            });

        });
    </script>

</body>
</html>